{
  "name": "keyboard-build-checker",
  "displayName": "Keyboard build checker",
  "description": "Checks the status of your builds in Azure devops.",
  "longDescription": "Checks the status of your builds in Azure devops.",
  "version": "1.0.0",
  "publisher": "Jeroen Knoester",
  "officialProductName": "Keyboard build checker",
  "appUrl": "",
  "isSingleton": false,
  "videoUrl": "",
  "icon": "assets/icon.png",
  "image": "assets/image-missing.png",
  "publisherName": "Jeroen Knoester",
  "authorName": "Jeroen Knoester",
  "authorUrl": "https://github.com/jeroenknoester/keyboard-build-checker/issues",
  "issuesUrl": "https://github.com/jeroenknoester/keyboard-build-checker/issues",
  "homePageUrl": "https://github.com/jeroenknoester/keyboard-build-checker",
  "developerRepoUrl": "https://github.com/jeroenknoester/keyboard-build-checker",
  "licenseUrl": "http://www.gnu.org/licenses/old-licenses/gpl-2.0.txt",
  "changelogUrl": "CHANGELOG.md",
  "license": "MIT",
  "readMeUrl": "README.md",
  "readMeEndUserUrl": "README_ENDUSER.md",
  "engines": {
    "das-keyboard-q": "3.2.4"
  },
  "main": "index.js",
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "daskeyboard-applet": "^2.11.1",
    "fs": "^0.0.1-security"
  },
  "qConfig": {
    "geometry": {
      "width": 4,
      "height": 1,
      "defaults": {
        "origin": {
          "x": 0,
          "y": 1
        }
      }
    },
    "questions": [
      {
        "key": "zoneId",
        "label": "PAT",
        "help": "Insert your Personal Access Token from Azure devops, so that the plugin can connect",
        "required": true,
        "order": 1,
        "controlType": "textbox",
        "options": []
      },
      {
        "key": "project_type",
        "label": "Project Type",
        "help": "Insert your Personal Access Token from Azure devops, so that the plugin can connect",
        "required": true,
        "order": 2,
        "controlType": "dropdown",
        "options": [{
          "key": "org",
          "value": "Organisation"
        }, {
          "key": "server",
          "value": "Server"
        }]
      },
      {
        "key": "instance",
        "label": "Instance",
        "help": "The Azure DevOps Services organization or TFS server you're sending the request to",
        "required": true,
        "order": 3,
        "placeholder":"my_instance_name",
        "controlType": "search",
        "options": []
      }
    ],
    "requirements": {
      "applet": {
        "zoneId": "select"
      }
    }
  },
  "devDependencies": {
    "mocha": "^5.2.0"
  }
}
